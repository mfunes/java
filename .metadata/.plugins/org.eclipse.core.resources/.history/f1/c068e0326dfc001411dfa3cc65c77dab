/**
 * @author Manuel Funes Domingo
 *
 * Menu de juegos
 * Podemos elegir jugar a: Monedas, Dados o Iching
 */

package menu;

public class Menu {
	
	Juego juego;
	public Menu(Juego juego) { this.juego = juego; }

	public void main(String[] args) {
		
		switch(juego) {
		
		case DADOS: new Menu(juego); break;
		case MONEDAS: new Menu(juego); break;
		case ICHING: new Menu(juego); break;
		
		}
		
		
	}
	
}
